<article{{ attributes }}>

  {{ title_prefix }}
    <h1{{ title_attributes }}>
     {% trans %}I'm <strong>Vincent Baronnet</strong>{% endtrans %}
    </h1>
  {{ title_suffix }}

  {% if display_submitted %}
    <footer>
      {{ author_picture }}
      <div{{ author_attributes }}>
        {% trans %}Submitted by {{ author_name }} on {{ date }}{% endtrans %}
        {{ metadata }}
      </div>
    </footer>
  {% endif %}

  <div{{ content_attributes }}>
    {{ content }}
  </div>

</article>
